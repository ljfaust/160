public class Candidate {
  
  private String name;
  private int votes;
  
//Default Constructor
  public Candidate(){
    name = " ";
    votes = 0;
  }
//Constructor with parameters  
  public Candidate (String name, int votes){
    this.name = name;
    this.votes = votes;
  }
  
//Name    
  public void setName(String name){
    this.name = name;
  }
  public String getName(){
    return name;
  }
  
//Votes  
  public void setVotes(int votes) {
    this.votes = votes;
  } 
  public int getVotes() {
   return votes;
  }  
  
}
  
